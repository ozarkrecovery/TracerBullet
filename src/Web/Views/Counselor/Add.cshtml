@model OzarkRecovery.Web.Models.CounselorAddModel

@{
    ViewBag.Title = "Add Counselor";
}

<h1>Add Counselor</h1>

@using (Html.BeginForm()) {

    <div class="editor-label">@Html.LabelFor(x => x.FirstName)</div>
    <div class="editor-field">@Html.EditorFor(x => x.FirstName)</div>

    @Html.ValidationSummary()

    @Html.SubmitButton("submit", "Add")
}
